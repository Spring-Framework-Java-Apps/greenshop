<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>
        
	<definition name="page" templateExpression="/WEB-INF/layout/page.jsp">
		<!-- header -->
		<put-attribute name="header" value="/WEB-INF/layout/header.jsp" />
		<put-attribute name="breadcrumb" value="/WEB-INF/layout/breadcrumb.jsp" />
		<!-- columns left -->
		<put-attribute name="categoryBox" value="/WEB-INF/layout/categoryBox.jsp" />
		<put-attribute name="manufacturersBox" value="/WEB-INF/layout/manufacturersBox.jsp" />
		<put-attribute name="quickFindBox" value="/WEB-INF/layout/quickFindBox.jsp" />
		<put-attribute name="newProductsBox" value="/WEB-INF/layout/newProductsBox.jsp" />
		<put-attribute name="informationPagesBox" value="/WEB-INF/layout/informationPagesBox.jsp" />
		<!-- columns right -->
		<put-attribute name="shoppingCartBox" value="/WEB-INF/layout/shoppingCartBox.jsp" />
		<put-attribute name="specialsBox" value="/WEB-INF/layout/specialsBox.jsp" />
		<put-attribute name="reviewsBox" value="/WEB-INF/layout/reviewsBox.jsp" />
		<put-attribute name="shareProductBox" value="/WEB-INF/layout/emptyBox.jsp" />
		<put-attribute name="manufacturerInfoBox" value="/WEB-INF/layout/emptyBox.jsp" />
		<put-attribute name="setNotificationBox" value="/WEB-INF/layout/emptyBox.jsp" />
		<put-attribute name="writeReviewBox" value="/WEB-INF/layout/emptyBox.jsp" />
		<put-attribute name="languagesBox" value="/WEB-INF/layout/languagesBox.jsp" />
		<put-attribute name="currenciesBox" value="/WEB-INF/layout/currenciesBox.jsp" />
		<!-- footer -->
		<put-attribute name="footer" value="/WEB-INF/layout/footer.jsp" />
		<put-attribute name="banner" value="/WEB-INF/layout/banner.jsp" />

	</definition>
        
</tiles-definitions>
